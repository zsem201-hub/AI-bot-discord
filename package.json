{
  "name": "discord-ai-bot",
  "version": "2.0.0",
  "description": "Powerful Discord AI Bot with Multi-Model, Music, TTS, and 20+ Skills",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "pm2": "pm2 start src/index.js --name ai-bot",
    "pm2:restart": "pm2 restart ai-bot",
    "pm2:stop": "pm2 stop ai-bot",
    "pm2:logs": "pm2 logs ai-bot",
    "pm2:monit": "pm2 monit",
    "setup": "node scripts/setup.js",
    "update": "git pull && npm install && pm2 restart ai-bot",
    "clean": "node scripts/cleanup.js",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "chatbot",
    "music",
    "lavalink",
    "tts",
    "gemini",
    "groq",
    "openrouter"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zsem201-hub/AI-bot-discord"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
