    "@discordjs/voice": "^0.17.0",
    "@discordjs/opus": "^0.9.0",
    "sodium-native": "^4.0.4",
    "prism-media": "^1.3.5",
    "ffmpeg-static": "^5.2.0",
    
    "shoukaku": "^4.1.0",
    "kazagumo": "^3.4.0",
    
    "@google/generative-ai": "^0.21.0",
    "groq-sdk": "^0.8.0",
