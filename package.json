    "sharp": "^0.33.5",
    "canvas": "^2.11.2",
    "node-html-to-image": "^4.0.0",
    
    "express": "^4.21.0",
    "bull": "^4.16.0",
    "uuid": "^10.0.0",
    "ms": "^2.1.3",
